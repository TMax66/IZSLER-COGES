@article{ ISI:000185516000017,
Author = {De Marco, MA and Foni, GE and Campitelli, L and Raffini, E and Di Trani,
   L and Delogu, M and Guberti, V and Barigazzi, G and Donatelli, I},
Title = {{Circulation of influenza viruses in wild waterfowl wintering in Italy
   during the 1993-99 period: Evidence of virus shedding and seroconvcrsion
   in wild ducks}},
